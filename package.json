{
  "name": "compression",
  "version": "1.0.0",
  "description": "compression program",
  "main": "window.js",
  "scripts": {
    "start": "electron .",
    "build-all": "electron-packager . --out=output --asar --overwrite --all",
    "build-mac": "electron-packager . --out=mac --asar --overwrite --platform=darwin --arch=x64 --icon=images/icons/star_full.icns",
    "build-window": "electron-packager . --out=window --asar --prune --platform=win32 --arch=x64",
    "build_window_installer": "node installer-window.js"
  },
  "author": "Hyunsoo Shin",
  "license": "MIT",
  "devDependencies": {
    "big-number": "^1.0.0",
    "electron": "^2.0.3",
    "electron-builder": "^20.15.1",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.6.4"
  },
  "dependencies": {
    "electron-installer-squirrel-windows": "^1.3.0"
  },
  "keywords": [
    "compression"
  ]
}
